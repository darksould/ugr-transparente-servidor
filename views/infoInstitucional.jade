mixin item(valor)
	.li
		a(class="enlaces-cabecera" href="#{valor.link}",target="_blank") #{valor.item}

	

extends layout
include menu.jade

block content
	#rastro-idiomas
		#rastro
			ul#rastro_breadcrumb
				li.first
					span.first #{seccion}
	#general
		+ menu("Información Institucional")
		#pagina
			h1#titulo_pagina
				span.texto_titulo #{seccion}

			div(id="contenido", class="sec_interior")
				.content_doku

					h1
						a(name="__doku_encabezado_de_primer_nivel") Rectora Magnífica de la Universidad de Granada:

					ul.enlaces
						li.level1
							.li
								a(class="enlaces-cabecera" href="http://www.ugr.es/pages/gobierno/rectoramagnificadelauniversidaddegranada",target="_blank") D.ª Pilar Aranda Ramírez

					h1
						a(name="__doku_encabezado_de_primer_nivel") Enlaces de interés:
					ul.enlaces
						li.level1
							.li
								a(class="enlaces-cabecera" href="http://rectorado.ugr.es/pages/equipo-gobierno",target="_blank") Equipo de Gobierno
							.li
								a(class="enlaces-cabecera" href="http://www.juntadeandalucia.es/boja/2015/151/BOJA15-151-00001-13759-01_00074790.pdf",target="_blank") Orden de suplencias de la Rectora
							.li
								a(class="enlaces-cabecera" href="http://secretariageneral.ugr.es/pages/normativa/fichasugr/boja15197000051647001_00077437/!",target="_blank") Delegación de competencias de la Rectora
							.li
								a(class="enlaces-cabecera" href="http://www.ugr.es/~defensor/",target="_blank") Defensor Universitario
							.li
								a(class="enlaces-cabecera" href="http://secretariageneral.ugr.es/pages/actascg/2015/actascg120315/documentos/calendacademico20152016/!",target="_blank") Calendario académico
							.li
								a(class="enlaces-cabecera" href="http://calidad.ugr.es/unidades",target="_blank") Cartas de Servicio y Contratos Programa
							.li
								a(class="enlaces-cabecera" href="http://calidad.ugr.es/pages/politica/cartelpoliticacalidadugrcontrastefecha",target="_blank") Política de Calidad
							.li
								a(class="enlaces-cabecera" href="http://secretariageneral.ugr.es/pages/memorias/academica",target="_blank") Memorias Académicas
							.li
								a(class="enlaces-cabecera" href="http://secretariageneral.ugr.es/pages/memorias/gestion",target="_blank") Memorias de Gestión
